<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.drp.entity.OOrderSeqEntity" table="o_order_seq" schema="drp">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="identity"/>
        </id>
        <property name="date">
            <column name="date" sql-type="int(8)"/>
        </property>
        <property name="seq">
            <column name="seq" sql-type="int(4)"/>
        </property>
        <property name="createBy">
            <column name="create_by" sql-type="int(11)"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="timestamp"/>
        </property>
        <property name="lastUpdateBy">
            <column name="last_update_by" sql-type="int(11)"/>
        </property>
        <property name="lastUpdateTime">
            <column name="last_update_time" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>