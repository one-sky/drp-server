<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.drp.entity.ORefundReasonCodeEntity" table="o_refund_reason_code" schema="drp">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="identity"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar(128)" length="128"/>
        </property>
        <property name="createBy">
            <column name="create_by" sql-type="int(11)"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="timestamp"/>
        </property>
        <property name="lastUpdateBy">
            <column name="last_update_by" sql-type="int(11)"/>
        </property>
        <property name="lastUpdateTime">
            <column name="last_update_time" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>